
struct ImageMessage
{
    int32_t frame_id;
    double timestamp;
    int32_t width;
    int32_t height;
    int32_t encoding;
    int32_t buffer_size;
    int8_t buffer[buffer_size];
}

struct MotorCommandMessage
{
    int8_t fullstop;
    double steering;
    double speed;
}

struct SelfDrivingMessage
{
    int8_t enable;
}

